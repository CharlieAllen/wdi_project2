<p id="notice"><%= notice %></p>

<h2>People.</h2>

  <ul>
    <% @users.each do |user| %>
        <li><%= link_to user.username, user%></li> 
        <li><%= image_tag user.user_image.thumb.url %></li> 
        <li><%= user.full_name %></li>
        <li><%= user.location.name if user.location %></li>
      </ul>
    <% end %>


<br>



<p id="notice"><%= notice %></p>

<h2>Pictures.</h2><br>
<%= link_to 'Add a picture', new_photo_path %>

  <ul>
    <% @photos.each do |photo| %>
        <li><%= photo.title %></li>
        <li><%= Location.find(photo.location_id).name %></li>
        <li><%= link_to image_tag(photo.image.thumb.url), photo %></li>
        </li>
      </ul>
    <% end %>




